   <div data-role="page" id="AddDelete">

      <div data-role="header" data-theme='b'>
         <h1>Bingo</h1>
         <div data-role="navbar">
            <ul>
               <li><a href="#Home" data-icon="home">Look for Pets</a></li>
               <li><a href="#Details" data-icon="star">Saved Pet List</a> </li>
               <li><a href="#Show" data-icon="star">Your Profile</a> </li>
            </ul>
         </div><!-- /navbar -->
      </div>


      <div data-role="content" data-theme='d'>

         <select id="invItems">
            <option value="filter">Filter</option>
            <option value="starFragchoice">Edit filter</option>

         </select>
         <!-- Tester Menus -->
         <section id=starfrags>
            <select id="starfragtype">
               <option value="Breed1">Breed 1</option>
               <option value="Breed2">Breed 2</option>
               <option value="Breed3">Breed 3</option>
               <option value="Breed4">Breed 4</option>
               <option value="Breed5">Breed 5</option>
            </select>

            <!-- Tester Menus -->
            <select id="starfragtype">
               <option value="Age">Age</option>
               <option value="Baby">Baby</option>
               <option value="Young">Young</option>
               <option value="Adult">Adult</option>
               <option value="Senior">Senior</option>

            </select>

            <!-- Tester Menus -->
            <select id="starfragtype">
               <option value="location">Location</option>
               <option value="Bamboo">Seattle</option>
               <option value="Bamboo Shoot">Renton</option>
               <option value="Spring Bamboo">Bellevue</option>
               <option value="Spring Bamboo">Bellevue</option>
            </select>
            <button id="starfragtype">Add!</button>
      </div>
      <!-- Stephanie's tester card, needs to be put in the same view div in order to be put on a specific area-->
      <div class="bingo">
         <div class="bingo--status">
            <i class="fa fa-remove"></i>
            <i class="fa fa-heart"></i>
         </div>
         <!-- Yes or no buttons. data-inline allows the buttons to be displayed side by side, buttons are on top for now because the card does not size properly -->
         <div class="bingo--buttons">
            <button id="nope" data-role="button" data-inline="true"><i class="fa fa-remove"></i></button>
            <button id="love" data-role="button" data-inline="true"><i class="fa fa-heart"></i></button>
         </div>

         <div class="bingo--cards">
            <div class="bingo--card">
               <img
                  src="https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/smartest-dog-breeds-lead-1587757081.jpg?crop=1.00xw:1.00xh;0,0&resize=640:*">
               <h3>Molly</h3>
               <p>Age: 2 years<br>
                  Gender: Female<br>
                  Breed: Corgi<br>
                  Location: Seattle</p>
               <p id = "dogId">1234567890</p>
            </div>
            <div class="bingo--card">
               <img
                  src="https://www.washingtonpost.com/resizer/uwlkeOwC_3JqSUXeH8ZP81cHx3I=/arc-anglerfish-washpost-prod-washpost/public/HB4AT3D3IMI6TMPTWIZ74WAR54.jpg">
               <h3>Piper</h3>
               <p>Age: 8 years<br>
                  Gender: Male<br>
                  Breed: Mixed<br>
                  Location: Seattle</p>
            </div>
            <div class="bingo--card">
               <img src="https://www.cesarsway.com/wp-content/uploads/2020/10/Screen-Shot-2020-10-31-at-2.11.21-PM.png">
               <h3>Woody</h3>
               <p>Age: 12 years<br>
                  Gender: Male<br>
                  Breed: Golden Retriever<br>
                  Location: Bellevue</p>
            </div>
            <div class="bingo--card">
               <img
                  src="https://media1.popsugar-assets.com/files/thumbor/QYcQ17h-c7xh-evZHmaGSGI1l-U/0x159:2003x2162/fit-in/2048xorig/filters:format_auto-!!-:strip_icc-!!-/2019/08/07/875/n/24155406/9ffb00255d4b2e079b0b23.01360060_/i/Cute-Pictures-German-Shepherd-Puppies.jpg">
               <h3>Lily</h3>
               <p>Age: 12 weeks<br>
                  Gender: Female<br>
                  Breed: German Shepherd<br>
                  Location: Bellevue</p>
            </div>
            <div class="bingo--card">
               <img src="https://www.pitbullinfo.org/uploads/7/8/9/7/7897520/pitbull-dog-1_3.jpg">
               <h3>Buddy</h3>
               <p>Age: 3 years<br>
                  Gender: Male<br>
                  Breed: Pit Bull<br>
                  Location: Redmond</p>
            </div>
            <script src='https://hammerjs.github.io/dist/hammer.min.js'></script>
         </div>

         <div data-role="footer" data-theme='b'>
            <h1>Team Bingo 2020</h1>
         </div>

      </div>



