   <div data-role="page" id="Show">

      <div data-role="header" data-theme='b'>
         <h1>Bingo</h1>
         <div data-role="navbar">
            <ul>
               <li><a href="#Show" data-icon="star">Search for Dogs</a> </li>
               <li><a href="#AddDelete" data-icon="plus">Saved Dogs</a> </li>
            </ul>
         </div><!-- /navbar -->
      </div>

      <div data-role="content" data-theme='c'>
         <h2>My Inventory</h2>
         <div>
            <ul id="listUl">

            </ul>

